<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="lk.dialog.touchfuel" version="1.0.3" xmlns="http://www.w3.org/ns/widgets"
        xmlns:gap="http://phonegap.com/ns/1.0" versionCode="1">
  <name>Touch Fuel</name>
  <description>
    An Ionic Framework and Cordova project.
  </description>
  <author email="hi@ionicframework" href="http://ionicframework.com/">
    Ionic Framework Team
  </author>
  <preference name="disable-push-notifications" value="1" />
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="android-installLocation" value="auto"/>

  <preference name="phonegap-version" value="cli-6.3.0"/>
  <!--    <preference name="webviewbounce" value="false"/>
      <preference name="UIWebViewBounce" value="false"/>-->
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <!--<preference name="StatusBarOverlaysWebView" value="true" />-->

  <!--  <feature name="StatusBar">
      <param name="ios-package" value="CDVStatusBar" onload="true"/>
    </feature>-->

  <content src="index.html"/>
  <access origin="*"/>
  <access origin="maps:*" launch-external="yes"/>
  <access origin="tel:*" launch-external="yes"/>
  <access origin="mailto:*" launch-external="yes"/>


  <!--<gap:plugin name="org.apache.cordova.inappbrowser" />-->
  <plugin name="cordova-plugin-device" spec="1.1.3"/>
  <plugin name="cordova-plugin-device-orientation" spec="1.0.4"/>
  <plugin name="cordova-plugin-dialogs" spec="1.3.0"/>
  <plugin name="cordova-plugin-file" spec="4.3.0"/>
  <plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator" spec="3.2.1" />
  <!--<gap:plugin name="com.phonegap.plugin.statusbar"/>-->
  <plugin name="cordova-plugin-geolocation" spec="1.0.1"/>
  <plugin name="cordova-plugin-network-information" spec="1.0.1"/>
  <plugin name="cordova-plugin-splashscreen" spec="4.0.1"/>

  <icon platform="ios" src="iphone57x57.png" width="57" height="57"/>
  <icon platform="ios" src="iphone60x60.png" width="60" height="60"/>
  <icon platform="ios" src="iphone72x72.png" width="72" height="72"/>
  <icon platform="ios" src="iphone76x76.png" width="76" height="76"/>
  <icon platform="ios" src="iphone114x114.png" width="114" height="114"/>
  <icon platform="ios" src="iphone120x120.png" width="120" height="120"/>
  <icon platform="ios" src="iphone144x144.png" width="144" height="144"/>
  <icon platform="ios" src="iphone152x152.png" width="152" height="152"/>
  <icon platform="ios" src="iphone180x180.png" width="180" height="180"/>


  <!--<icon src="splash.png" platform="ios"/>-->

  <!--<gap:splash src="splash.png"/>-->

  <splash platform="ios" src="Default~iphone.png" width="320" height="480" />
  <splash platform="ios" src="Default@2x~iphone.png" width="640" height="960" />
  <splash platform="ios" src="Default-568h@2x~iphone.png" width="640" height="1136" />
  <splash platform="ios" src="Default-667h.png" width="750" height="1334" />
  <splash platform="ios" src="Default-736h@3x~iphone.png" width="1242" height="2208" />
  <splash platform="ios" src="Default-Portrait~ipad.png" width="768" height="1024" />
  <splash platform="ios" src="Default-Portrait@2x~ipad.png" width="1536" height="2048" />


  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="2000"/>
  <preference name="orientation" value="portrait" />
  <preference name="FadeSplashScreenDuration" value="750"/>

  <preference name="ShowSplashScreenSpinner" value="false" />
  <!--<preference name="AutoHideSplashScreen" value="false" />-->
  <!--<preference name="auto-hide-splash-screen" value="false" />-->


</widget>

